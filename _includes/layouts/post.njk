---
layout: layouts/base.njk
section: post
---

<article class='post-body'>
  <div class='title-header'>
    <div class='text-title-header'>
      <h1>{{ title }}</h1>
      <h5>{{ subject }}</h5>
      <p>{{ hookText | markdown | safe}}</p>
    </div>
    <img src="{{ image }}"/>
  </div>
  {{ layoutContent | safe }}
</article>

<nav>
  <a href="{{ '/' | url }}">‚Üê More Interviews</a>
</nav>
